<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="rootPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="360.0" minWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.aaronwilson.cuepad.controller.RootController">
   <top>
      <BorderPane styleClass="header" BorderPane.alignment="CENTER">
         <left>
            <HBox alignment="CENTER" spacing="10.0" BorderPane.alignment="CENTER">
               <children>
                  <Button disable="true" mnemonicParsing="false" text="Load" />
                  <Button disable="true" mnemonicParsing="false" text="Save" />
                  <Region />
                  <Button fx:id="newScene" mnemonicParsing="false" text="New Scene" />
                  <Button mnemonicParsing="false" onAction="#clearGrid" text="Clear Scene" />
               </children>
            </HBox>
         </left>
         <right>
            <HBox alignment="CENTER" spacing="10.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="previousScene" disable="true" mnemonicParsing="false" text="&lt;" />
                  <Label fx:id="sceneName" alignment="CENTER" maxWidth="100.0" minWidth="100.0" text="Scene" textAlignment="CENTER" />
                  <Button fx:id="nextScene" disable="true" mnemonicParsing="false" text="&gt;" />
               </children>
            </HBox>
         </right>
         <padding>
            <Insets bottom="20.0" left="30.0" right="30.0" top="20.0" />
         </padding>
      </BorderPane>
   </top>
</BorderPane>
