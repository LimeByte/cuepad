<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="rootPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="360.0" minWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.aaronwilson.cuepad.controller.RootController">
   <top>
      <BorderPane styleClass="header" BorderPane.alignment="CENTER">
         <left>
            <HBox alignment="CENTER" spacing="20.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="manageScenes" mnemonicParsing="false" text="Manage Scenes" />
               </children>
            </HBox>
         </left>
         <right>
            <HBox alignment="CENTER" spacing="10.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="previousScene" disable="true" mnemonicParsing="false" text="&lt;" />
                  <Label fx:id="sceneName" alignment="CENTER" minWidth="100.0" text="Scene" />
                  <Button fx:id="nextScene" disable="true" mnemonicParsing="false" text="&gt;" />
               </children>
            </HBox>
         </right>
         <padding>
            <Insets bottom="15.0" left="30.0" right="30.0" top="15.0" />
         </padding>
         <center>
            <HBox alignment="CENTER" spacing="20.0" BorderPane.alignment="CENTER">
               <children>
                  <Slider blockIncrement="1.0" disable="true" minorTickCount="2" value="100.0" />
                  <Button fx:id="stop" mnemonicParsing="false" text="Stop" />
               </children>
            </HBox>
         </center>
      </BorderPane>
   </top>
</BorderPane>
